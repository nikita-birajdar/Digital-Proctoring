    <!-- Main content -->
       <section class="content">
      <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        
        <!-- /.row -->
        <!-- Main row -->
          <div class="card card-default">
          <div class="card-header">
            <h3 class="card-title">Add Student Information</h3>

            <div class="card-tools">
              <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus"></i></button>
              <button type="button" class="btn btn-tool" data-card-widget="remove"><i class="fas fa-remove"></i></button>
            </div>
          </div>
          <!-- /.card-header -->
          <div class="card-body">
            <div class="row">
              <div class="col-md-6">
			  <form action="/addstudentpersonalinfo" method="POST" enctype="multipart/form-data">
                <div class="form-group">
                  <label>Select Department</label>
                  <select name="department"class="form-control select2" style="width: 100%;">
                    <option value="null">Select Department</option>	
                    <option value="MCA1">MCA1</option>	  			  		
	<option value="MCA2">MCA2</option>	
<option value="MCA3">MCA3</option>

                  </select>
                </div>
                <!-- /.form-group -->
                <div class="form-group">
                  <label>Student USN</label>
                  <input type="text" name="studentid"class="form-control" readonly value="<%= records %>" placeholder="Enter Student USN" required >
                </div>

                <div class="form-group">
                  <label>Student Name</label>
                  <input type="text" name="studentname"class="form-control" readonly value="<%= sn %>" placeholder="Enter Student Name" required >
                </div>
                <!-- /.form-group -->
              </div>
			  
              <div class="form-group">
                <label>Semester</label>
                <input type="text" name="semester"class="form-control" placeholder="Enter Semester" required >
              </div>
              <!-- /.col -->
             <div class="col-md-6">
              <div class="form-group">
                  <label>Phone</label>
                  <input type="text" name="phonenumber"class="form-control" placeholder="Enter Phone" required >
                </div>
                <!-- /.form-group -->
             <div class="form-group">
                  <label>Email</label>
             <input type="text" name="email"class="form-control" placeholder="Enter Email" required >
                </div>
                <!-- /.form-group -->
              </div>
			  
			   <div class="col-md-6">
              <div class="form-group">
                  <label>DOB</label>
                  <input type="date" name="DOB"class="form-control"  required >
                </div>

                <div class="form-group">
                  <label>Select Gender</label>
                  <select name="gender" class="form-control select2"  style="width: 100%;">
                    <option value="Male" selected="selected">Male</option>
                    <option value="Female" >Female</option>
                   
                  </select>
                </div>
                <!-- /.form-group -->
             <div class="form-group">
                  
          <div class="form-group">
                  <label>Enter Address</label>
             <textarea name="address" class="form-control" >  </textarea >
                </div>
                </div>
                <!-- /.form-group -->
              </div>
			  
			   <div class="col-md-6">
              <div class="form-group">
                  <label>10 th Marks</label>
                  <input type="text" name="tength"class="form-control" placeholder="Enter 10th marks" required >
                </div>
                <!-- /.form-group -->
               <div class="form-group">
                  <label>12 th Marks</label>
                  <input type="text" name="twelfth"class="form-control" placeholder="Enter 12th marks" required >
                </div>
                <!-- /.form-group -->
              </div>
			     
				 <div class="col-md-6">
              <div class="form-group">
                  <label>CGPA</label>
                  <input type="text" name="cgpa"class="form-control" placeholder="Enter 10th marks" required >
                </div>
                <!-- /.form-group -->
               <div class="form-group">
                  <label>Resume</label>
                  <input type="file" name="resume"class="form-control" placeholder="Enter Student Name" required >
                </div>
                <!-- /.form-group -->
              </div>
			 
              <div class="form-group">
                <label>Proctor ID</label>
                <input type="text" name="proctorid"class="form-control" readonly value="<%= pi %>" placeholder="Enter Proctor Id" required >
              </div>
              <!-- /.col -->
            </div>
            <!-- /.row -->
            <div class="form-group">
              <label>Proctor Name</label>
              <input type="text" name="proctorname"class="form-control" readonly value="<%= pn %>" placeholder="Enter Proctor Name" required >
            </div>
			  
          
              </div>
			   
			 
			   <button type="submit" class="btn btn-block bg-gradient-success ">Send  Information</button>
			  </form>
            </div>
			
            <!-- /.row -->
          </div>
          <!-- /.card-body -->
          
